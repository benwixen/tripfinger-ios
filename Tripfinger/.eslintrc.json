{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-use-before-define": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-else-return": "off",
        "no-trailing-spaces": ["warn", { "skipBlankLines": true }],
        "react/prefer-stateless-function": "off",
        "react/jsx-filename-extension": "off",
        "react/forbid-prop-types": "off"
    }
}
